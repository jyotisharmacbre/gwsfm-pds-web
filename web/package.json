{
    "name": "cbre-pds-web",
    "version": "0.1.0",
    "private": true,
    "jest": {
        "coverageReporters": [
            "cobertura",
            "html",
            "lcov"
        ]
    },
    "jest-junit": {
        "suiteName": "jest tests",
        "output": "test/junit.xml",
        "classNameTemplate": "{classname} - {title}",
        "titleTemplate": "{classname} - {title}",
        "ancestorSeparator": " > ",
        "usePathForSuiteName": "true"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.25",
        "@fortawesome/free-solid-svg-icons": "^5.11.2",
        "@fortawesome/react-fontawesome": "^0.1.7",
        "@material-ui/core": "^4.3.2",
        "@material-ui/icons": "^4.2.1",
        "@types/lodash": "^4.14.145",
        "@types/redux-form": "^8.1.9",
        "@microsoft/applicationinsights-react-js": "^2.3.1",
        "@microsoft/applicationinsights-web": "^2.3.1",
        "@types/node": "12.7.1",
        "@types/react": "16.9.0",
        "@types/react-datepicker": "^2.9.2",
        "@types/react-dom": "16.8.5",
        "@types/react-redux": "^7.1.2",
        "@types/react-router-dom": "^5.1.3",
        "@types/react-test-renderer": "^16.9.0",
        "@types/redux-logger": "^3.0.7",
        "@types/redux-mock-store": "^1.0.1",
        "airbnb-js-shims": "^2.2.1",
        "axios": "^0.19.0",
        "axios-progress-bar": "^1.2.0",
        "core-js": "^3.6.4",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "file-saver": "^2.0.2",
        "lodash": "^4.17.13",
        "moment": "^2.24.0",
        "msal": "^1.2.1",
        "preval.macro": "^4.0.0",
        "react": "^16.9.0",
        "react-aad-msal": "^2.3.3",
        "react-app-polyfill": "^1.0.6",
        "react-bootstrap": "^0.31.5",
        "react-bootstrap-table-next": "^3.3.5",
        "react-bootstrap-table2-paginator": "^2.1.0",
        "react-bootstrap-typeahead": "^3.4.7",
        "react-datepicker": "^2.9.6",
        "react-dates": "^21.5.0",
        "react-dom": "^16.9.0",
        "react-intl": "^3.1.11",
        "react-moment": "^0.9.7",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-toastify": "^5.4.1",
        "recharts": "^1.8.5",
        "redux": "^4.0.4",
        "redux-form": "^8.2.6",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.3",
        "redux-thunk": "^2.3.0",
        "typescript": "^3.4.5",
        "url-search-params-polyfill": "^8.0.0",
        "xlsx": "^0.15.5",
        "jwt-decode": "^2.2.0"
    },
    "scripts": {
        "build-css": "node-sass --include -path scss src/scss/style.scss src/css/style.css",
        "watch-css": "nodemon -e scss -x \"npm run build-css\"",
        "pretty": "prettier -l \"src/**/*.tsx\" \"src/**/*.ts\" --write",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "build:ci": "npm run build-css && react-scripts build --nomaps",
        "start-js": "react-scripts start",
        "eject": "react-scripts eject",
        "test": "react-scripts test",
        "intl": "cross-env NODE_ENV=development extract-messages --locales=en --output src/Translations/extracted 'src/Translations/messages.ts'",
        "test:watch": "npm run test -- --watch",
        "test:ci": "react-scripts test --env=jsdom --testResultsProcessor=\"jest-junit\" --coverage --watchAll=false"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "IE 11"
    ],
    "devDependencies": {
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "24.0.17",
        "check-prop-types": "^1.1.2",
        "cross-env": "^5.2.0",
        "extract-react-intl-messages": "^1.1.0",
        "node-sass": "^4.13.0",
        "nodemon": "^1.19.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "react-test-renderer": "^16.11.0",
        "nock": "^11.7.0",
        "jest-junit": "^9.0.0"
    }
}
